-- 작업 1
-- DM 발송목록 테이블
create table DM_TBL (
  dmno number(8) not null,
  custid varchar2(10),
  maildate date,
  contents varchar2(13),
  dept char(2),
  grade char(1),
  campain char(2),
  constraint dm_pk primary key (dmno)
);

-- 작업 2
-- 고객정보 테이블
create table customer_tbl (
  custid varchar2(10) not null,
  author varchar2(20),
  goods char(2),
  phone varchar2(13),
  email varchar2(20),
  area varchar2(20),
  constraint customer_pk primary key (custid)
);

-- 작업 3
